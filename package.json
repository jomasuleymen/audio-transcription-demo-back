{
	"name": "audio-transcription",
	"version": "0.0.1",
	"main": "index.ts",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc && tsup",
		"start:dev": "node --import=tsx --watch src/index.ts",
		"start:prod": "node dist/index.js",
		"check": "biome check --write"
	},
	"dependencies": {
		"@apollo/server": "^5.0.0",
		"@as-integrations/express5": "^1.1.2",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "5.1.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/express": "5.0.3",
		"@types/node": "^24.3.0",
		"tsconfig-paths": "^4.2.0",
		"tsup": "8.5.0",
		"tsx": "4.20.5",
		"typescript": "^5.9.2",
		"@graphql-tools/load-files": "^7.0.1",
		"@graphql-tools/merge": "^9.1.1"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2022",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	}
}